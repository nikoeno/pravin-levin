---
import SanyaNix from "../assets/nix.jpg";
const { class: logoClass, ...restProps } = Astro.props;
---

<img
  class:list={["logo", logoClass]}
  src={SanyaNix.src}
  alt=""
  {...restProps}
/>
<style>
  .logo {
    aspect-ratio: 1/1;
    height: 175px;
    width: 175px;
    display: block;
    border-radius: 50%;
    object-fit: cover;
    object-position: top;
  }
</style>
