---
import SanyaNix from "../assets/nix.jpg";
const { class: logoClass, ...restProps } = Astro.props;
---

<img
  class:list={["logo", logoClass]}
  src={SanyaNix.src}
  alt=""
  {...restProps}
/>
<style>
  .logo {
    height: 150px;
    width: 150px;
    display: block;
    border-radius: 50%;
    object-fit: cover;
  }
</style>
