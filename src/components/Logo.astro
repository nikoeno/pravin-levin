---
import SanyaNix from "../assets/nix.jpg";
const { class: logoClass, ...restProps } = Astro.props;
---

<img
  class:list={["logo", logoClass]}
  src={SanyaNix.src}
  alt=""
  {...restProps}
/>
<style>
  .logo {
    height: 150px;
    width: 150px;
    display: block;
    /* border-radius: 50%; */
    border-radius: 2rem;
    /* border-radius: 3rem; */
    object-fit: cover;
    object-position: top;
  }
</style>
